@import './index.styl'
a
  color #666
a.active
    color $color1
    border-bottom 2px solid $color1
.head
    height 1.4rem
    // background $color1
.top
    width 100%
    top 0
    border-bottom .7px solid #f0f0f1
    .top_bg
        width 100%
        height 1rem
        background $color1
        position relative
        .tit
            color #fff
            font-size 22px
            position absolute
            top .3rem
            left .3rem
.select
    display flex
    height .4rem
    justify-content space-around
    background #fff
    line-height .4rem
    .col
        flex 1
        text-align center
    a 
        // flex 1
        // display inline-block
        padding-bottom .03rem
        text-align center
        font-size 16px